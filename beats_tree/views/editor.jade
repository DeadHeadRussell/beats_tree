doctype html
html
  head
    meta(charset='utf-8')
    title Beats Tree
    link(rel='stylesheet', href='/css/app.css')
    script(src='/js/jquery/dist/jquery.js')
    script(src='/js/editor.js')
  body
    div
      h1= title
      script(src='/js/timbre.js')
      script.
        if (typeof beat_tree == 'undefined') {
          var beat_tree = {};
        }
        beat_tree.tempo = #{data.tempo};
        beat_tree.track_ids = #{data.track_ids};
      p= data.tempo
      #grid
      div(ng-repeat='track in tracks')
        .track {{track.content}}

